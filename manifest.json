{
    "manifest_version": 2,

    "name": "TLS Stats",
    "description": "Collects stats about which hosts are the worst TLS offenders",
    "version": "1.0",

    "background": {
        "scripts": ["collector.js"]
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "storage",
        "unlimitedStorage",
        "webRequest",
        "<all_urls>"
    ]
}
